{
  "version": "0.1.2",
  "license": "MIT",
  "description": "Compose middleware for Next.js API routes like React Hooks",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "next",
    "nextjs",
    "middleware",
    "api",
    "api",
    "routes",
    "api routes"
  ],
  "scripts": {
    "build": "rollup -c && tsc",
    "prepublish": "build",
    "dev": "next dev -p 3000",
    "test": "cypress run",
    "test:watch": "cypress open",
    "size": "size-limit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint && pretty-quick --staged"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "3 kB"
    }
  ],
  "name": "next-api-mw",
  "author": "teagueStockwell",
  "module": "dist/next-api-mw.esm.js",
  "devDependencies": {
    "@open-wc/building-rollup": "^2.0.1",
    "@size-limit/preset-app": "^6.0.4",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "cypress": "^9.0.0",
    "husky": "^7.0.4",
    "next": "^12.0.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^6.0.4",
    "typescript": "^4.4.4",
    "yup": "^0.32.11",
    "rollup": "^2.60.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsAppDevelopment/next-api-mw"
  },
  "bugs": {
    "url": "https://github.com/tsAppDevelopment/next-api-mw/issues"
  },
  "homepage": "https://github.com/tsAppDevelopment/next-api-mw",
  "dependencies": {
  }
}
