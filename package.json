{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsdx build",
    "dev": "next dev -p 3000",
    "test": "cypress run",
    "test:watch": "cypress open",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17",
    "react-dom": "^16.13.1 || ^17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint && pretty-quick --staged"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "name": "next-api-mw",
  "author": "teagueStockwell",
  "module": "dist/next-api-mw.esm.js",
  "size-limit": [
    {
      "path": "dist/next-api-mw.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/next-api-mw.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.4",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "cypress": "^9.0.0",
    "husky": "^7.0.4",
    "next": "^12.0.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^6.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "yup": "^0.32.11"
  }
}
